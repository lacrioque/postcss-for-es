{
  "name": "postcss-for-es",
  "version": "1.0.2",
  "description": "PostCSS plugin that enables SASS-like for loop syntax in your CSS - now in typescript and es6",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "for",
    "loop",
    "sass",
    "from",
    "to",
    "iterate",
    "nested"
  ],
  "module": "dist/postcss-for-es.mjs",
  "main": "dist/postcss-for-es.js",
  "types": "dist/postcss-for-es.d.ts",
  "author": "Markus Fl√ºr <markusfluer@markusfluer.de>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lacrioque/postcss-for-es.git"
  },
  "dependencies": {
    "postcss": "^8.4.47",
    "postcss-simple-vars": "^7.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@vitest/ui": "^2.1.4",
    "eslint": "^9.13.0",
    "globals": "^15.11.0",
    "postcss-custom-properties": "^14.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.12.1",
    "vite": "^5.4.10",
    "vite-plugin-dts": "^4.3.0",
    "vitest": "^2.1.4"
  },
  "scripts": {
    "build": "vite build",
    "test": "vitest",
    "test:ci": "vitest --watch false --outputFile=.test/test-results.junit --reporter=junit",
    "test:viz": "vitest --watch false --outputFile=.test/test-results.html --reporter=html",
    "lint": "eslint ."
  }
